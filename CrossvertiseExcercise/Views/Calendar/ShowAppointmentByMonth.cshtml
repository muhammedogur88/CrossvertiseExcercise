@model List<CrossvertiseExcercise.ViewModels.AppointmentListViewModel>

<ul class="bordered">
    @if (Model?.Count > 0)
    {
        foreach (var item in Model)
        {
            <li appointmentId="@item.Id">
                <span>@item.Date</span> <a href="javascript:;" data-item-id="@item.Id">@item.Description</a>
            </li>
        }

    }
</ul>